; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Planner"
run/main_scene="res://Pages/Splash.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Luneal Labs/Planner"
boot_splash/image="res://Assets/png/blank.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

GLOBAL="*res://GLOBAL.gd"

[display]

window/size/width=1440
window/size/height=816
window/size/resizable=false
window/size/borderless=true
window/energy_saving/keep_screen_on=false
mouse_cursor/custom_image="res://Assets/png/mouse/cursor_pointerFlat.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[input]

left_click={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[locale]

test="en"
translations=PoolStringArray( "res://Assets/Translation.en.translation", "res://Assets/Translation.fr.translation" )
locale_filter=[ 1, [ "en", "fr" ] ]

[memory]

limits/message_queue/max_size_kb=2000

[rendering]

environment/default_clear_color=Color( 0.921569, 0.643137, 1, 1 )
environment/default_environment="res://default_env.tres"
